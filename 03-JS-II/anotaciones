//WHILE

var i= 0;                           
while(i < 100){
    console.log(i);
i++;}                               
// siempre logear el 1++ (si no se crea bucle infinito)



var j = 0;                           
while( j < 100){
    console.log(j);
}   



function matriz(n, m){
    var j = 0;
    var i = 0;
    while (i < n){
        while(j < m){
            console.log(i, j);
            j = j +1;
        }
        j = 0;
        i = i +1;
    }
}


