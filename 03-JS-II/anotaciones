       ejemplo // function imprime 1 a N         
       function imprime1aN(n) {
        for(var i = 1; i <= n; i++){
            console.log(i);
        }
       }

 // se detiene en 200 por qe le doy otra condicion
        function imprime1aN(n){
        for(var i = 1; i <= n; i++)
        {if( i % 2 === 0){
            console.log(i)}
            if( i > 200) {                
                return;
            }
        }
    }


// dos condiciones // imprime del 1 al 200 (se detiene) y luego imprime del 1 al 9 y luego return

function imprime1aN(n){
    for(var i = 1; i <= n; i++)
    {if( i % 2 === 0){
        console.log(i)}
        if( i > 200) {                
        for(var j = 0; j < 10; j++){
            console.log(j);}
        
        return;}
    }
}
    
